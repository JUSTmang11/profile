            </div>
            <table>
                <tbody>
                    <% if(status){%>
                        <%for(let i=0; i < (Object.keys(user).length) ; i++){%>
                            <tr>
                                <td>
                                    <%= user[i].uid %>
                                </td>
                                <td>
                                    <%= user[i].name %>
                                </td>
                                <td>
                                    <%= user[i].charector %>
                                </td>
                                <td>
                                    <%= time %>
                                </td>
                                <td>
                                    <button onclick="toggleUI('<%=user[i].uid%>','<%=user[i].name%>','<%=user[i].charector%>')">More</button>
                                </td>
                            </tr>
                            <%}%>
                                <%}%>
                </tbody>
            </table>
            <% if(!status){%>
                <h1 style="text-align: center;
                    margin: .3em 0 0 0;">NOT FOUND</h1>
                <%}%>
        </div>
        <div class="avg-ch c-i"></div>
    </main>
